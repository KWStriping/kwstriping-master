{
  "name": "@tempo/dashboard",
  "private": true,
  "type": "module",
  "scripts": {
    "_dev": "npx kill-port $PORT_DASHBOARD; dotenv -e ../../.env -- next dev --port $PORT_DASHBOARD --hostname $DOMAIN",
    "build": "NEXTJS_IGNORE_TYPECHECK=1 NEXTJS_IGNORE_ESLINT=1 next build",
    "build:analyze": "ANALYZE=true pnpm build",
    "build:debug": "NEXTJS_IGNORE_TYPECHECK=1 NEXTJS_IGNORE_ESLINT=1 next build --debug",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf .next .turbo",
    "cypress": "cypress open",
    "debug": "pnpm predev; NODE_OPTIONS='--inspect' next dev --port $PORT_DASHBOARD",
    "dev": "npx dotenv-run-script ../../.env -- _dev",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql,.gql --cache --cache-location ../../.cache/eslint/dashboard.eslintcache",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql,.gql --fix",
    "lint:next": "next lint --dir .",
    "lint:staged": "next lint --dir . --fix --quiet",
    "paths": "pathpida --ignorePath ../../.gitignore",
    "start": "next start",
    "test": "jest",
    "test:record": "POLLY_MODE=record jest --runInBand --verbose",
    "typecheck": "tsc --noEmit"
  },
