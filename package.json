{
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "//postbuild": "next-sitemap",
    "_dev": "npx kill-port $PORT_STOREFRONT; next dev --turbo --port $PORT_STOREFRONT --hostname $DOMAIN",
    "//prebuild": "if [[ -z \"$DOCKER\" ]]; then tsx scripts/prebuild.ts; run-p codegen compile; fi",
    "build": "dotenv -- next build && pnpm postbuild",
    "build:analyze": "ANALYZE=true pnpm build",
    "postbuild": "test -d .next/standalone || ls .next/standalone",
    "clean": "rm -rf .next .turbo",
    "clean:deep": "pnpm clean; pnpm clean:deps",
    "clean:deps": "rm -rf node_modules; rm -rf **/node_modules; rm pnpm-lock.yaml; pnpm install",
    "cypress": "cypress open",
    "dev": "docker system prune -f && docker compose up -d --wait web && next dev --turbo",
    "dev:paths": "pathpida --ignorePath .gitignore --watch",
    "generate:paths": "pathpida --ignorePath .gitignore",
    "generate": "dotenv -e .env -c -- turbo run generate",
    "lint": "pnpm eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "dotenv -e .env -c -- turbo run lint:staged",
    "prepare": "if [ ! $DOCKER = true ]; then npx husky; fi",
    "start": "next start",
    "test": "jest",
    "test:record": "POLLY_MODE=record jest --runInBand --verbose",
    "typecheck": "tsc --noEmit",
    "compile": "tsx scripts/compile.ts",
    "postinstall": "pnpm compile",
    "//codegen": "pnpm generate --filter=tempo && graphql-codegen --config graphql.config.ts && graphql-codegen --config graphql.config.ts --project dashboard",
    "codegen": "graphql-codegen --config graphql.config.ts",
    "db:backup": "docker compose run --rm tempo python manage.py dbbackup",
    "db:restore": "docker compose run --rm tempo python manage.py dbrestore",
    "format": "prettier --write .",
    "please": "python tempo/manage.py"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-tailwindcss"
    ]
  },
