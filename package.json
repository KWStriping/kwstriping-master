{
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "@tempo",
    "apps/*",
    "tempo"
  ],
  "scripts": {
    "compile": "tsx scripts/compile.ts",
    "postinstall": "pnpm compile",
    "prebuild": "if [[ -z \"$DOCKER\" ]]; then tsx scripts/prebuild.ts; run-p codegen compile; fi",
    "build": "pnpm prebuild && turbo run build --log-prefix=none",
    "clean": "rm -rf .turbo; turbo run clean",
    "clean:deep": "pnpm clean; pnpm clean:deps",
    "clean:deps": "rm -rf node_modules; rm -rf **/node_modules; rm pnpm-lock.yaml; pnpm install",
    "//codegen": "pnpm generate --filter=tempo && graphql-codegen --config graphql.config.ts && graphql-codegen --config graphql.config.ts --project dashboard",
    "codegen": "graphql-codegen --config graphql.config.ts",
    "db:backup": "docker compose run --rm tempo python manage.py dbbackup",
    "db:restore": "docker compose run --rm tempo python manage.py dbrestore",
    "dev": "docker system prune -f && docker compose up -d --wait web && turbo run dev --log-prefix=none",
    "format": "prettier --write .",
    "generate": "dotenv -e .env -c -- turbo run generate",
    "lint": "dotenv -e .env -c -- turbo run lint",
    "lint:staged": "dotenv -e .env -c -- turbo run lint:staged",
    "please": "python tempo/manage.py",
    "prepare": "husky",
    "start": "dotenv -e .env -c -- turbo run start --parallel",
    "test": "env-cmd --no-override -f .env turbo run test",
    "typecheck": "turbo run typecheck"
  },
  "resolutions": {
    "graphql": "16.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "dependencies": {
    "@mui/material": "^5.16.14",
    "@mui/material-nextjs": "^5.16.14"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.4",
    "@graphql-codegen/client-preset": "^4.6.1",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript": "^4.1.3",
    "@graphql-codegen/typescript-resolvers": "^4.4.2",
    "@graphql-codegen/typescript-urql": "^4.0.0",
    "@graphql-codegen/urql-introspection": "^3.0.0",
    "@inlang/paraglide-js": "^1.11.8",
    "@next/eslint-plugin-next": "^14.2.23",
    "@rollup/plugin-typescript": "^11.1.6",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/google.maps": "^3.58.1",
    "@types/jest": "^29.5.14",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "20.12.12",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.9.0",
    "@typescript-eslint/parser": "7.9.0",
    "change-case": "^5.4.4",
    "change-case-all": "^2.1.0",
    "commander": "^12.1.0",
    "cypress": "^13.17.0",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^7.4.4",
    "dotenv-expand": "^11.0.7",
    "dotenv-run-script": "^0.4.1",
    "esbuild": "0.21.3",
    "eslint": "8.57.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-nibble": "^8.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "28.5.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-playwright": "1.6.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-rulesdir": "^0.2.2",
    "eslint-plugin-sonarjs": "1.0.3",
    "eslint-plugin-storybook": "0.8.0",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-testing-library": "^6.5.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "espree": "10.0.1",
    "globals": "^15.14.0",
    "graphql": "16.8.1",
    "graphql-config": "^5.1.3",
    "husky": "9.0.11",
    "jest": "^29.7.0",
    "json-merge-patch": "^1.0.2",
    "lint-staged": "15.2.2",
    "nginx-linter": "^0.6.3",
    "nginxbeautifier": "^1.0.19",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.22.0",
    "prettier": "^3.4.2",
    "prettier-plugin-multiline-arrays": "^3.0.6",
    "prettier-plugin-packagejson": "^2.5.8",
    "rimraf": "^5.0.10",
    "rollup": "^4.34.4",
    "syncpack": "^12.4.0",
    "tailwindcss": "3.4.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "turbo": "^2.4.0",
    "typescript": "5.4.5"
  },
  "packageManager": "pnpm@10.2.1",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=7.0.0",
    "pnpm": ">=10.2.1"
  },
  "pnpm": {
    "supportedArchitectures": {
      "cpu": [
        "x64",
        "arm64"
      ],
      "os": [
        "darwin",
        "linux"
      ]
    }
  }
}
