    "ts-node": "^10.9.2",
    "tsx": "^4.15.2",
    "turbo": "^2.0.3",
    "type-fest": "^4.20.0",
    "typescript": "5.4.5"
  },
  "packageManager": "pnpm@9.3.0",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=7.0.0",
    "pnpm": ">=9.1.2"
  },
  "pnpm": {
    "supportedArchitectures": {
      "cpu": [
        "x64",
        "arm64"
      ],
      "os": [
        "darwin",
        "linux"
      ]
    },
    "overrides": {
      "@opentelemetry/api": "1.8.0",
      "@opentelemetry/instrumentation": "0.51.1"
    }
  }
}
