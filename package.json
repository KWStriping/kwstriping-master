{
  "name": "root",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*",
    "tempo"
  ],
  "scripts": {
    "//codegen": "pnpm generate --filter=tempo && graphql-codegen --config graphql.config.ts && graphql-codegen --config graphql.config.ts --project dashboard",
    "prebuild": "if [ -f scripts/prebuild.ts ]; then dotenv -e .env -c -- ts-node-esm scripts/prebuild.ts; fi",
    "build": "pnpm prebuild && dotenv -e .env -c -- turbo run build --log-prefix=none",
    "clean": "rm -rf .turbo; turbo run clean",
    "codegen": "pnpm generate --filter=tempo && graphql-codegen --config graphql.config.ts",
    "db:backup": "docker compose run --rm tempo python manage.py dbbackup",
    "db:restore": "docker compose run --rm tempo python manage.py dbrestore",
    "deps:nuke": "rimraf **/node_modules",
    "dev": "docker system prune -f && docker compose up -d --wait web && turbo run dev --log-prefix=none",
    "format": "prettier --write .",
    "generate": "dotenv -e .env -c -- turbo run generate",
    "i18n:generate": "i18next",
    "lint": "dotenv -e .env -c -- turbo run lint",
    "lint:staged": "dotenv -e .env -c -- turbo run lint:staged",
    "please": "python tempo/manage.py",
    "prepare": "husky",
    "start": "dotenv -e .env -c -- turbo run start --parallel",
    "test": "env-cmd --no-override -f .env turbo run test",
    "typecheck": "turbo run typecheck"
  },
  "resolutions": {
    "@types/react": "^18.2.45",
    "graphql": "16.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "19.2.2",
    "@core/eslint-rules": "workspace:*",
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.2.5",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-resolvers": "^4.1.0",
    "@graphql-codegen/urql-introspection": "^3.0.0",
    "@next/eslint-plugin-next": "^14.2.1",
    "@swc-node/register": "^1.9.0",
    "@types/node": "18.13.0",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "change-case": "^5.3.0",
    "change-case-all": "^2.1.0",
    "cypress": "^13.10.0",
    "dotenv-cli": "^7.4.1",
    "dotenv-run-script": "^0.4.1",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-nibble": "^8.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-playwright": "1.6.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-rulesdir": "^0.2.2",
    "eslint-plugin-sonarjs": "0.25.0",
    "eslint-plugin-storybook": "0.8.0",
    "eslint-plugin-tailwindcss": "^3.17.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "espree": "10.0.1",
    "graphql": "16.8.1",
    "graphql-config": "^5.0.3",
    "husky": "9.0.11",
    "i18next-parser": "^8.13.0",
    "lint-staged": "15.2.2",
    "nginx-linter": "^0.6.3",
    "nginxbeautifier": "^1.0.19",
    "prettier": "^3.2.5",
    "prettier-plugin-multiline-arrays": "^3.0.4",
    "prettier-plugin-packagejson": "^2.5.0",
    "rimraf": "^5.0.5",
    "syncpack": "^12.3.0",
    "ts-node": "^10.9.2",
    "turbo": "^1.13.0",
    "typescript": "5.4.2"
  },
  "packageManager": "pnpm@9.0.6",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0",
    "pnpm": ">=9.0.6"
  }
}
