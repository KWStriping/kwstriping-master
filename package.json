{
  "name": "master",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "@tempo/dashboard",
    "apps/*",
    "tempo"
  ],
  "scripts": {
    "compile": "tsx scripts/compile.ts",
    "pnpm:devPreinstall": "",
    "preinstall": "tsx scripts/preinstall.ts",
    "//postinstall": "run-p codegen compile",
    "build": "tsx scripts/prebuild.ts && dotenv -e ./.env -- turbo run build --log-prefix=none --env-mode=loose",
    "clean": "rm -rf .turbo; turbo run clean",
    "clean:deep": "pnpm clean; pnpm clean:deps",
    "clean:deps": "rm -rf node_modules; rm -rf **/node_modules; rm pnpm-lock.yaml; pnpm install",
    "//codegen": "pnpm generate --filter=tempo && graphql-codegen --config graphql.config.ts && graphql-codegen --config graphql.config.ts --project dashboard",
    "codegen": "graphql-codegen --config graphql.config.ts",
    "db:backup": "docker compose run --rm tempo python manage.py dbbackup",
    "db:restore": "docker compose run --rm tempo python manage.py dbrestore",
    "dev": "tsx scripts/predev.ts && docker system prune -f && docker compose up -d --wait web && dotenv -e ./.env -- turbo run dev --log-prefix=none --env-mode=loose",
    "format": "prettier --write .",
    "generate": "dotenv -e .env -c -- turbo run generate",
    "lint": "dotenv -e .env -c -- turbo run lint",
    "lint:staged": "dotenv -e .env -c -- turbo run lint:staged",
    "please": "python tempo/manage.py",
    "prepare": "husky",
    "start": "dotenv -e .env -c -- turbo run start --parallel",
    "test": "env-cmd --no-override -f .env turbo run test",
    "typecheck": "turbo run typecheck"
  },
  "resolutions": {
    "@opentelemetry/api": "1.8.0",
    "@opentelemetry/instrumentation": "0.51.1",
    "graphql": "16.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "dependencies": {
    "graphql": "16.8.1",
    "lodash-es": "^4.17.21",
    "next": "14.2.4",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@graphql-codegen/add": "^5.0.2",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.6",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript": "^4.0.7",
    "@graphql-codegen/typescript-resolvers": "^4.1.0",
    "@graphql-codegen/typescript-urql": "^4.0.0",
    "@graphql-codegen/urql-introspection": "^3.0.0",
    "@inlang/paraglide-js": "^1.9.1",
    "@next/eslint-plugin-next": "^14.2.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/google.maps": "^3.55.9",
    "@types/jest": "^29.5.12",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "20.14.2",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.13.0",
    "@typescript-eslint/parser": "7.13.0",
    "change-case": "^5.4.4",
    "change-case-all": "^2.1.0",
    "commander": "^12.1.0",
    "cypress": "^13.11.0",
    "deep-object-diff": "^1.1.9",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "dotenv-expand": "^11.0.6",
    "dotenv-run-script": "^0.4.1",
    "eslint": "8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-nibble": "^8.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "28.6.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-playwright": "1.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-rulesdir": "^0.2.2",
